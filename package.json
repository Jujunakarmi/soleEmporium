{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "build": "cd client && npm i && npm run build && cp -r ./dist ../server/dist && cd ../server && npm i ",
        "server": "cd server && npm run watch",
        "client": "cd client && npm run start",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "start": "cd server && cross-env NODE_ENV=production node server.js"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^6.2.2",
        "cross-env": "^7.0.3",
        "vite": "^5.3.3"
    }
}
